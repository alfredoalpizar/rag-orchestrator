# DeepSeek API Configuration
DEEPSEEK_API_KEY=sk-your-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_CHAT_MODEL=deepseek-chat
DEEPSEEK_REASONING_MODEL=deepseek-reasoner
DEEPSEEK_EMBEDDING_MODEL=deepseek-embedding-v2

# Fireworks AI Configuration (for Qwen models)
FIREWORKS_API_KEY=fw-your-key-here
FIREWORKS_BASE_URL=https://api.fireworks.ai/inference/v1
QWEN_THINKING_MODEL=accounts/fireworks/models/qwen3-235b-a22b-thinking-2507
QWEN_INSTRUCT_MODEL=accounts/fireworks/models/qwen3-235b-a22b-instruct-2507
QWEN_EMBEDDING_MODEL=accounts/fireworks/models/nomic-embed-text-v1.5

# ChromaDB Configuration
CHROMADB_BASE_URL=http://localhost:8000
CHROMADB_COLLECTION=knowledge_base

# Database Configuration (PostgreSQL) - Only needed when using database mode
# DB_URL=jdbc:postgresql://localhost:5432/rag_orchestrator
# DB_USERNAME=postgres
# DB_PASSWORD=your_password

# Loop Configuration
LOOP_MAX_ITERATIONS=10
LOOP_MAX_TOKENS=4096
LOOP_USE_REASONING_MODEL=false
LOOP_MODEL_STRATEGY=qwen_single_thinking
LOOP_THINKING_SHOW_REASONING=true
LOOP_STREAMING_SHOW_REASONING_TRACES=true

# Conversation Storage (in-memory is default, no database required)
# CONVERSATION_STORAGE_MODE=database

# S3 Archive (Optional)
# ARCHIVE_S3_BUCKET=conversations-archive
 AWS_ACCESS_KEY_ID=dummy
 AWS_SECRET_ACCESS_KEY=dummy
 AWS_REGION=us-east-1

# Spring Profile
SPRING_PROFILES_ACTIVE=dev
